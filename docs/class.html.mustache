<!DOCTYPE HTML>
<html>

<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <title>Lazy.js API Docs - {{name}}</title>
  <link rel="stylesheet" type="text/css" href="docs.css" />
  <script type="text/javascript" src="../site/lib/jquery.js"></script>
  <script type="text/javascript" src="../site/common.js"></script>
</head>

<body id="top">
  <header>
    <h1><a href="../index.html">lazy.js</a></h1>
  </header>

  <article>
    <header>
      <h1>
        {{name}}
        <small><a href="index.html">Back</a></small>
      </h1>
    </header>

    <nav class="doc-list" data-sections="section.method">
      <ul>
        {{#methods}}
        <li><a href="#{{name}}">{{name}}</a></li>
        {{/methods}}
      </ul>
    </nav>

    <section class="methods">
      {{#methods}}
      <section id="{{name}}" class="method" style="display: none;">
        <header>
          <h2>
            {{name}}
            <small>{{longname}}</small>
          </h2>
          <p class="description">{{{description}}}</p>
        </header>

        <h3>Parameters</h3>
        {{#params}}
        <table class="params">
          <tr>
            <th>Name</th>
            <th>Type</th>
            <th>Description</th>
          </tr>

          {{#list}}
          <tr>
            <td>{{name}}</td>
            <td>{{type}}</td>
            <td>{{{description}}}</td>
          </tr>
          {{/list}}
        </table>
        {{/params}}
        {{^params}}
        <p>None</p>
        {{/params}}

        <h3>Returns</h3>
        {{#returns}}
        <table class="returns">
          <tr>
            <th>Type</th>
            <th>Description</th>
          </tr>

          {{#list}}
          <tr>
            <td>{{type}}</td>
            <td>{{{description}}}</td>
          </tr>
          {{/list}}
        </table>
        {{/returns}}
        {{^returns}}
        <p>Nothing</p>
        {{/returns}}

        <h3>Source</h3>
        <pre><code class="javascript">{{{source}}}</code></pre>
      </section>
      {{/methods}}
    </section>
  </article>
</body>

</html>
