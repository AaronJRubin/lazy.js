<section id="benchmark-results-{{id}}"
  {{^selected}}style="display: none;"{{/selected}}
  class="benchmark-results-section">

  <div id="benchmark-results-chart-{{id}}" class="column-chart"
    data-title="Ops/second - {{label}}"
    data-source="#benchmark-results-table-{{id}}"
    data-transpose="true"
    data-row-filter="benchmarkChartRowFilter"
    data-options="benchmarkChartOptions">
  </div>

  <p>
    Select:
    <button class="select-all">All</button>
    <button class="select-none">None</button>
    <button class="clear-selected">Clear</button>
  </p>

  <table id="benchmark-results-table-{{id}}" class="benchmark-results-table">
    <tr>
      <th>Test</th>
      <th>Underscore</th>
      <th>Lo-Dash</th>
      <th>Lazy.js</th>
    </tr>
  </table>
</section>
